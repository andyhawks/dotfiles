if [ "$OS" == "Darwin" ]; then

  # homebrew - /usr/local/bin is in /etc/paths
  pathmunge /usr/local/sbin

  # MAMP config
  pathmunge /Applications/MAMP/bin/php/php5.2.17/bin
  pathmunge /Applications/MAMP/Library/bin

  # Android SDK
  pathmunge /Users/david/Sites/eclipse/android-sdk-mac_86

  # Ruby Gems
  pathmunge /Users/david/.gem/ruby/1.8/bin
fi
